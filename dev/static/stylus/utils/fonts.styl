@font-face {
  font-family: 'Montserrat 600';
  src: local('Montserrat Semi Bold'), local('Montserrat-Semi-Bold'),
          url('/lk/static/fonts/Montserrat-SemiBold.woff2') format('woff2'),
          url('/lk/static/fonts/Montserrat-SemiBold.woff') format('woff'),
          url('/lk/static/fonts/Montserrat-SemiBold.ttf') format('truetype');
  font-weight: 600;
  font-style: normal;
}

