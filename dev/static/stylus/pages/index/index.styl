.profile-chnge-img
    position: absolute
    left: 0
    right 0
    top: 0
    bottom: 0
    line-height: 0
    opacity 0
    width 100%
    cursor pointer
    z-index 2

.profile-section-top__img-change
    &:hover
        .profile-change-placeholder
            opacity 1
.profile-change-placeholder
    display flex
    flex-direction: column
    justify-content: center
    align-items: center
    position: absolute
    left: 0
    right 0
    top: 0
    bottom: 0
    line-height: 0
    background-color: rgba(0,0,0,0.5)
    width 100%
    border-radius: 10px
    opacity 0
    transition: all linear .2s

    .icon
        size(30px)
        margin-bottom: 13px
        fill: #fff
    span
        color #fff
        font-family: RB400
        font-size: 13px
