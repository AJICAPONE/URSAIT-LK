    justify-content: center
    align-items center
    background-color: #2F80ED
    border-radius: 10px
    size(34px)
    transition: all linear .2s
    margin-right: 25px
    .icon
        display flex
        justify-content: center
        size(18px)
        fill: #fff
    &:last-child
        margin-right: 0

    &:hover
        box-shadow: 0 0 8px 1px #2f80eda1;
.profile-section-top__connect
    display flex
.profile-section-top--right
    display flex
    align-items: center
.section-contacts--items
    display flex
    margin-bottom: 25px
