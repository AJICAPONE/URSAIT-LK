    font-family: MS600
    font-size: 13px
    color: #272727
    cursor pointer
    padding 10px 15px
    transition: all linear .2s
    border-radius: 5px
    margin-right: 10px
    &:hover
        background-color: #2F80ED
        box-shadow: 0 0 8px 1px #2f80eda1;
        color #fff
.bg-white
    background-color: #fff
    // border-radius: 10px
.blue-color
    background-color: #2F80ED
    color: #fff
    box-shadow: 0 0 8px 1px #2f80eda1;
.panel-logout
    display flex
    align-items: center
    justify-content: center
    height 36px
    font-family: MS600
    font-size: 13px
    color: #FF4B55
    cursor pointer
    padding 10px 15px
    transition: all linear .2s
    border-radius: 5px
    &:hover
        background-color: #FF4B55
        box-shadow: 0 0 8px 1px #FF4B55;
        color #fff
.section-top
    display flex
    justify-content: space-between
    margin-bottom: 30px
    padding-bottom: 30px
