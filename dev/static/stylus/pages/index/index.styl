.sidebar-content
    display flex
    flex-direction: column
    justify-content: space-between
    //background-color: #fff
    border-radius: 10px
    height 100%
.sidebar-menu-item
    display flex
    align-items: center
    padding: 20px
    &:hover
        background-color: #F9F9F9
        span
            font-family: MS600
            color #272727
    &:first-child
        border-radius: 10px 10px 0 0

    .icon
        size(21px)
        margin-right: 15px
